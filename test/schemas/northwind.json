{
  "$database": {
    "default": "conn2"
  },
  "category": {
    "model": {
      "CategoryID": "Number",
      "Description": "String",
      "CategoryName": "String"
    },
    "options": {
      "collection": "categories",
      "autoIncrement": {
          "field": "CategoryID",
          "startValue": 0,
          "step": 2
      }
    }
  },
  "customer": {
    "model": {
      "City": "String",
      "Fax": "String",
      "PostalCode": "String",
      "ContactTitle": "String",
      "Phone": "String",
      "ContactName": "String",
      "CustomerID": "String",
      "Country": "String",
      "CompanyName": "String",
      "Region": "String",
      "Address": "String"
    },
    "options": {
      "collection": "Customers"
    }
  }
}
